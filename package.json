        "concurrently": "^5.2.0",
        "nodemon": "^2.0.4",
        "prettier": "2.0.5"
    },
    "dependencies": {
        "@hapi/joi": "^17.1.1",
        "@material-ui/lab": "^4.0.0-alpha.56",
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "dotenv": "^8.2.0",
        "express": "^4.16.3",
        "if-env": "^1.0.4",
        "jwt-decode": "^2.2.0",
        "mongoose": "^5.9.20",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "react-swipeable-views": "^0.13.9"
